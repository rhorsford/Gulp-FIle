{
  "stylingPath": {
    "src": [
      "app/assets/**/*.scss",
      "!app/assets/**/_variables.scss"
    ],
    "dest": "app/public/",
    "watch": [
      "app/assets/**/*.scss",
      "app/assets/**/**/*.scss"
    ],
    "assets": "app/assets/",
    "docsAssets": "docs/assets/",
    "govukModules": "govuk_modules/",
    "assetsFrontend": "assets_modules/",
    "nodeModules": "node_modules/",
    "lib": "lib/"
  },
  "zipPath": {
    "included": [
      "./app/public/*",
      "./app/public/**/**"
    ],
    "name": "assets.zip"
  },
  "jsPath": {
    "src": [
      "app/assets/**/*.js",
      "app/assets/**/**/*.js"
    ],
    "dest": "app/public/"
  },
  "imagePath": {
    "src": [
      "app/assets/**/*.png",
      "app/assets/**/**/*.png"
    ],
    "dest": "app/public/",
    "watch": "app/assets/**/*"
  },
  "cleanupPath": {
    "dest": "app/public/"
  },
  "version": {
    "src": ""
  },
  "order": {
    "jsStructure": [
      "app/assets/src/javascripts/test1.js",
      "app/assets/src/javascripts/test.js",
      "app/assets/src/javascripts/**/*.js"
    ]
  }
}
